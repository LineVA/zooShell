<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath doyenm.zooshell.Main</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -classpath %classpath doyenm.zooshell.Main</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath doyenm.zooshell.Main</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-clean install</actionName>
            <displayName>clean install</displayName>
            <goals>
                <goal>clean</goal>
                <goal>install</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-sonar</actionName>
            <displayName>sonar</displayName>
            <goals>
                <goal>mvn</goal>
                <goal>sonar:sonar</goal>
                <goal>\</goal>
                <goal>-Dsonar.host.url=https://sonarcloud.io</goal>
                <goal>\</goal>
                <goal>-Dsonar.organization=lineva-github</goal>
                <goal>\</goal>
                <goal>-Dsonar.login=zoo-token</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-report</actionName>
            <displayName>report</displayName>
            <goals>
                <goal>mvn</goal>
                <goal>report</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-surefire</actionName>
            <displayName>surefire</displayName>
            <goals>
                <goal>surefire-report:report</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-codeCoverage</actionName>
            <displayName>codeCoverage</displayName>
            <goals>
                <goal>clean</goal>
                <goal>install</goal>
                <goal>site</goal>
                <goal>-P</goal>
                <goal>test</goal>
            </goals>
        </action>
    </actions>
